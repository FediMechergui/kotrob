{
  "expo": {
    "name": "لعبة الجذور",
    "slug": "jidhr-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./falcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./falcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5E6D3"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./falcon.png",
        "backgroundColor": "#F5E6D3"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fedimechergui.jidhrgame"
    },
    "web": {
      "favicon": "./falcon.png"
    },
    "plugins": [["expo-localization"], "expo-font"],
    "extra": {
      "eas": {
        "projectId": "bb9a2e1d-dc91-44eb-a780-91647e3b1c27"
      }
    }
  }
}
